{
    "presets": [
        "es2015",
        "react"
    ],
    "plugins": [
        "transform-class-properties",
        "transform-object-rest-spread",
    ],
    "env": {
        "test": {
            "plugins": [
                "babel-plugin-espower"
            ]
        },
        "production": {
            "plugins": [
                [
                    "transform-imports", {
                    "reactstrap": {
                        "transform": "reactstrap/lib/${member}",
                        "preventFullImport": true
                    }
                }],
                "transform-react-remove-prop-types"
            ]
        }
    }
}

